{
  "name": "react-starter",
  "description": "ZebraX React Starter Project using React, GraphQL, Apollo Client",
  "keywords": [
    "react",
    "graphql",
    "apollo-client"
  ],
  "scripts": {
    "analyze": "webpack --config scripts/webpack/webpack.config.analysis.js --progress",
    "build": "webpack --config scripts/webpack/webpack.config.prod.js --color -p --hide-modules --display-optimization-bailout",
    "build:clean": "rimraf ./dist",
    "build:storybook": "build-storybook -o dist/storybook",
    "dev": "webpack-dev-server --config scripts/webpack/webpack.config.dev.js --hot --host 0.0.0.0",
    "docker:build-prod": "docker-compose -f scripts/docker/docker-compose.prod.yaml build",
    "docker:build-storybook": "docker-compose -f scripts/docker/docker-compose.storybook.yaml build",
    "docker:build-test": "docker-compose -f scripts/docker/docker-compose.test.yaml build",
    "docker:clean": "docker rmi $(docker images -q -f 'dangling=true' -f 'label=autodelete=true')",
    "docker:run-test": "docker-compose -f scripts/docker/docker-compose.test.yaml run --rm react-starter-test",
    "lint": "eslint ./src/*.ts{,x} --fix",
    "lint:staged": "eslint --ignore-path .gitignore --fix",
    "serve:docs": "npx http-server docs",
    "serve:storybook": "npx http-server dist/storybook",
    "storybook": "start-storybook -p 6006 -s ./public",
    "test": "cross-env NODE_ENV=test jest --config scripts/jest.config.js",
    "test:coverage": "cross-env NODE_ENV=test jest --config scripts/jest.config.js --collectCoverage",
    "tsc": "tsc -p . --noEmit"
  },
  "author": "Florence Tjandra <florence.tjandra@zebrax.id>",
  "main": "index.js",
  "license": "MIT",
  "dependencies": {
    "@apollo/client": "^3.1.3",
    "@date-io/moment": "^1.3.11",
    "@material-ui/core": "^4.9.12",
    "@material-ui/lab": "^4.0.0-alpha.51",
    "@material-ui/pickers": "^3.2.10",
    "@material-ui/styles": "^4.9.10",
    "@reduxjs/toolkit": "^1.3.6",
    "@sentry/browser": "5.12.1",
    "@zebraxid/frontend-kit": "0.2.9",
    "classnames": "^2.2.6",
    "connected-react-router": "^6.8.0",
    "formik": "^2.1.4",
    "formik-material-ui": "^2.0.1",
    "gaugeJS": "^1.3.7",
    "google-map-react": "^1.1.7",
    "graphql": "^14.6.0",
    "graphql-tag": "^2.10.3",
    "highcharts": "^7.2.0",
