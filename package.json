  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "yarn lint:staged",
      "git add --force"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
